@model IEnumerable<WebApp.Models.Post>

<table>
    <tr><th>Title</th><th>Description</th><th>Author</th><th>Enable</th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Title</td>
            <td>@item.Description</td>
            <td>@item.Author</td>
        </tr>
    }
</table>

<a asp-controller="Home" asp-action="Index">Index</a>