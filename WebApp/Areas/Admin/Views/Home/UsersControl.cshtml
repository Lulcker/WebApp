@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>

<table>
    <tr><th>UserName</th><th>Email</th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td>@item.Email</td>
        </tr>
    }
</table>

<a asp-controller="Home" asp-action="Index">Index</a>